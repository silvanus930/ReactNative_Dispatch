{
  "name": "dispatch",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "env:dev": "ENVFILE=config/dev/.env ENV=development ./config/config.sh",
    "env:test": "ENVFILE=config/test/.env ENV=test ./config/config.sh",
    "env:prod": "ENVFILE=config/prod/.env ENV=production ./config/config.sh",
    "android": "yarn env:dev && react-native run-android --appIdSuffix dev --variant=devDebug",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "TZ=UTC jest",
    "lint": "eslint 'src/**/*.ts' 'src/**/*.tsx' --quiet --fix",
    "push": "yarn lint && yarn test && git push",
    "ios:pod": "cd ./ios && pod update && pod install --repo-update",
    "ios:pod-clean": "cd ./ios && pod cache clean --all && rm -rf Pods",
    "ios:internal": "yarn ios:pod && bundle exec fastlane ios internal",
    "ios:beta": "yarn ios:pod && bundle exec fastlane ios beta",
    "ios:prod": "yarn ios:pod && bundle exec fastlane ios prod",
    "ios:local-prod": "yarn env:prod && react-native run-ios --scheme 'ViaApp release'",
    "ios:run": "yarn env:prod && react-native run-ios",
    "ios:run-4.7": "yarn ios:run --simulator='iPhone 6s'",
    "ios:run-5.5": "yarn ios:run --simulator='iPhone 8 Plus'",
    "ios:run-6.5": "yarn ios:run --simulator='iPhone 13 Pro Max'",
    "ios:run-12.9": "yarn ios:run --simulator='iPad Pro (12.9-inch) (3rd generation)'",
    "ios:run-12.9.2": "yarn ios:run --simulator='iPad Pro (12.9-inch) (2nd generation)'",
    "ios:xcode": "cd ./ios && xcodebuild -project ViaApp.xcodeproj -target ViaApp -showBuildSettings",
    "android:avd": "adb devices",
    "android:avd-all": "emulator -list-avds",
    "android:clean": "cd android && ./gradlew clean && cd .. && rm -rf ./android/app/src/main/res/drawable-*/",
    "android:connect": "adb reverse tcp:8081 tcp:8081",
    "android:internal": "yarn env:dev && bundle && bundle exec fastlane android internal",
    "android:beta": "yarn env:test && bundle && bundle exec fastlane android beta",
    "android:prod": "yarn env:prod && bundle && bundle exec fastlane android prod",
    "android:local-dev": "yarn android",
    "android:local-test": "yarn env:test && react-native run-android --appIdSuffix test --variant=stagingDebug",
    "android:local-prod": "yarn env:prod && react-native run-android --variant=prodDebug",
    "android:local-prod-pay": "yarn env:prod && react-native run-android --variant=prodRelease",
    "android:log": "adb logcat",
    "android:pixel": "emulator -avd Pixel_4_XL_API_31",
    "clear": "watchman watch-del-all && rm -rf node_modules && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-* && yarn cache clean && yarn install",
    "postinstall": "patch-package",
    "icon": "iconset create assets/icon.png"
  },
  "dependencies": {
    "@native-base/icons": "^0.0.11",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/datetimepicker": "^6.3.4",
    "@react-native-community/netinfo": "^9.3.0",
    "@react-navigation/bottom-tabs": "^6.3.3",
    "@react-navigation/material-bottom-tabs": "^6.2.3",
    "@react-navigation/material-top-tabs": "^6.2.3",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "@react-navigation/stack": "^6.2.3",
    "@reduxjs/toolkit": "^1.8.5",
    "axios": "^0.27.2",
    "date-fns": "^2.29.2",
    "deprecated-react-native-prop-types": "^2.3.0",
    "formik": "^2.2.9",
    "grid-flatlist-react-native": "^0.1.8",
    "jwt-decode": "^3.1.2",
    "native-base": "^3.4.14",
    "react": "18.2.0",
    "react-native": "0.70.1",
    "react-native-compressor": "https://github.com/Shobbak/react-native-compressor#cc04c945df1609c3d5773be08aaf975a9ef93b8b",
    "react-native-fast-image": "^8.6.1",
    "react-native-file-viewer": "^2.1.5",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.4.2",
    "react-native-image-marker": "^0.6.3",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-lightbox": "^0.8.1",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-pager-view": "^5.4.25",
    "react-native-paper": "^4.12.4",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.17.0",
    "react-native-share": "^7.9.0",
    "react-native-sound": "^0.11.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.1.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-use-sound": "^1.2.2",
    "react-native-vector-icons": "^9.2.0",
    "react-native-vision-camera": "^2.14.1",
    "react-navigation-tabs": "^2.11.2",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "use-subscription": "^1.8.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/runtime": "^7.18.9",
    "@react-native-community/eslint-config": "^3.1.0",
    "babel-jest": "^29.0.0",
    "eslint": "^8.22.0",
    "jest": "^29.0.0",
    "metro-react-native-babel-preset": "^0.72.1",
    "react-test-renderer": "18.2.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
